{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/live-data.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test-python.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/portfolio-vercel.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/trading-engine.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auto-trader.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/live-bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/backtest-sync.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/live-data",
      "dest": "/api/live-data.py"
    },
    {
      "src": "/api/test-python",
      "dest": "/api/test-python.py"
    },
    {
      "src": "/api/portfolio",
      "dest": "/api/portfolio-vercel.py"
    },
    {
      "src": "/api/trading-engine",
      "dest": "/api/trading-engine.py"
    },
    {
      "src": "/api/auto-trader",
      "dest": "/api/auto-trader.py"
    },
    {
      "src": "/api/live-bot",
      "dest": "/api/live-bot.py"
    },
    {
      "src": "/api/backtest-sync",
      "dest": "/api/backtest-sync.py"
    },
    {
      "src": "/api/backtest-sync/update",
      "dest": "/api/backtest-sync.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
