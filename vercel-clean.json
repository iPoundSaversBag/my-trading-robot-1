{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/live-data.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/live-data",
      "dest": "/api/live-data.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ],
  "functions": {
    "api/live-data.py": {
      "runtime": "python3.9"
    }
  }
}
